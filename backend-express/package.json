{
    "name": "mental-health-backend-express",
    "version": "1.0.0",
    "description": "MERN Backend for Mental Health Application",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --forceExit --detectOpenHandles",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --forceExit",
        "test:comprehensive": "jest tests/api.comprehensive.test.js --forceExit --verbose",
        "test:mega": "jest tests/mega.test.js --forceExit --verbose --maxWorkers=4",
        "test:all": "jest --forceExit --verbose --maxWorkers=4"
    },
    "keywords": [
        "mental-health",
        "MERN",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.5.1",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.6.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^7.0.12",
        "socket.io": "^4.7.2",
        "twilio": "^5.11.1"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ],
        "testTimeout": 30000
    }
}
